{
  "name": "test-tube",
  "version": "0.0.1",
  "description": "Try something new here",
  "main": "./lib",
  "repository": "git@github.com:albburtsev/test-tube.git",
  "author": "Alexander Burtsev",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "clean": "rm -rf lib",
    "compile": "yarn run clean && tsc --pretty -p ."
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@types/ramda": "^0.25.37",
    "ramda": "^0.25.0"
  }
}
